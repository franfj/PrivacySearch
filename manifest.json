{
	
  "name": "PrivacySearch",
  "description" : "To use, type 'privacy' plus a search term into the Omnibox.",
  "version": "1.0",
  
  "background": {
    "scripts": [
	    "js/wordnet/data.js",
	    "js/wordnet/index.js",
	    "js/wordnet/lemmas.js",
	    "js/wordnet/stopwords.js",
	    "js/src/constants.js",
	    "js/src/preProcess.js",
	    "js/src/singularize.js",
	    "js/src/wsd.js",
		"popup.js",
		"background.js"]
  },
  
  "browser_action": {
      "default_popup": "popup.html"
  },
  
  "permissions": [
	  "activeTab",
      "storage"
  ],
  
  "omnibox": { "keyword" : "privacy" },
  
  "manifest_version": 2
  
}
